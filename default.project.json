{
    "name": "Magic Zombie Mayhem",
    "tree": {
        "$className": "DataModel",

        "Workspace": {
			"$className": "Workspace",
			"$ignoreUnknownInstances": true,
			"$path": "src/Workspace"
		},

        "Lighting": {
			"$className": "Lighting",
			"$ignoreUnknownInstances": true,
			"$path": "src/Lighting"
		},

        "ReplicatedStorage": {
            "$className": "ReplicatedStorage",
            "$ignoreUnknownInstances": true,
            "$path": "src/ReplicatedStorage",

            "Submodules": {
                "$className": "Folder",
                "Core": {
                    "$className": "Folder",
                    "Client": {
                        "$className": "Folder",
                        "$path": "Submodules/Core/Source/Client"
                    }
                }
            }
        },
        
        "ServerScriptService": {
            "$className": "ServerScriptService",
            "$path": "src/ServerScriptService",

            "Submodules": {
                "$className": "Folder",
                "Core": {
                    "$className": "Folder",
                    "Server": {
                        "$className": "Folder",
                        "$path": "Submodules/Core/Source/Server"
                    }
                }
            }
        },

        "StarterPlayer": {
            "$className": "StarterPlayer",
            "$properties": {
                "LoadCharacterAppearance": false,
                "CharacterWalkSpeed": 20,
                "CharacterJumpPower": 0,
                "CharacterUseJumpPower": false,
				"EnableMouseLockOption": false
			},
            
            "StarterCharacterScripts": {
                "$className": "StarterCharacterScripts",
                "$ignoreUnknownInstances": true,
                "$path": "src/StarterPlayer/StarterCharacterScripts"
            },
            "StarterPlayerScripts": {
                "$className": "StarterPlayerScripts",
                "$ignoreUnknownInstances": true,
                "$path": "src/StarterPlayer/StarterPlayerScripts"
            }
        }
    }
}